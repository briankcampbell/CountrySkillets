<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.poweredbyrita.countryskillets" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/country_skillets_logo_75x75"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VendorsActivity"
            android:label="@string/title_activity_vendors" >
        </activity>
        <activity
            android:name=".InventoryActivity"
            android:label="@string/title_activity_inventory" >
        </activity>
    </application>

</manifest>
